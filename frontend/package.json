  "dependencies": {
    "@babel/preset-env": "^7.24.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.2",
    "@mui/x-date-pickers": "^6.10.2",
    "@mui/x-date-pickers-pro": "^6.10.2",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.7",
    "babel-jest": "^29.7.0",
    "dayjs": "^1.11.9",
    "firebase": "^10.1.0",
    "framer-motion": "^10.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.14.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "selenium-webdriver": "^4.18.1",
    "ts-jest": "^29.1.2",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "set CI='' && react-scripts build",
    "test": "vitest --exclude=./src/tests/testE2E/* ", 
    "test-e2e": "mocha ./src/tests/testE2E/*.test.js",
    "eject": "react-scripts eject",
    "e2e-tests": "cypress run",
    "stop": "kill $(lsof -t -i:3000)",
    "ci": "start-server-and-test start http://localhost:3000 'mocha ./src/tests/*.test.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-react": "^7.23.3",
    "@storybook/testing-react": "^2.0.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@vitejs/plugin-react": "^4.2.1",
    "chai": "^5.1.0",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "mocha": "^10.3.0",
    "react-test-renderer": "^18.2.0",
    "serve": "^14.2.1",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.3.3",
    "vitest": "^1.3.1"
  }
